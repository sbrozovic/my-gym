<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/suset_sea_group"
    tools:context=".Modules.Members.OnlyMember.Fragments.HomeMemberFragment">

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#80000000"/>
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipeToRefreshHomeMemberFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imageViewWorkOutInMember"
                android:layout_width="154dp"
                android:layout_height="97dp"
                android:layout_marginBottom="0dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/viewOverManWorkingOutInMember"
                app:layout_constraintLeft_toLeftOf="@+id/viewOverManWorkingOutInMember"
                app:srcCompat="@drawable/man_working_out" />

            <View
                android:id="@+id/viewOverManWorkingOutInMember"
                android:layout_width="154dp"
                android:layout_height="97dp"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="32dp"
                android:background="#80000000"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewTariff" />

            <TextView
                android:id="@+id/textViewMembersInGym"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="42dp"
                android:layout_marginEnd="45dp"
                android:layout_marginRight="52dp"
                android:text="0"
                android:textColor="#fff"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/viewOverManWorkingOutInMember"
                app:layout_constraintRight_toRightOf="@+id/viewOverManWorkingOutInMember" />

            <TextView
                android:id="@+id/textViewCoachInMember"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="8dp"
                android:fontFamily="casual"
                android:text="@string/coach"
                android:textColor="#fff"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageViewWorkOutInMember" />

            <TextView
                android:id="@+id/textViewCoachNameIMember"
                android:layout_width="wrap_content"
                android:layout_height="34dp"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:fontFamily="casual"
                android:textColor="#fff"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toRightOf="@+id/textViewCoachInMember"
                app:layout_constraintTop_toBottomOf="@+id/imageViewWorkOutInMember" />

            <TextView
                android:id="@+id/textViewName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="24dp"
                android:text="@string/name"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewMyGymTitle" />

            <TextView
                android:id="@+id/textViewSurname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="20dp"
                android:text="@string/surname"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewName" />

            <TextView
                android:id="@+id/textViewMembershipAU"
                android:layout_width="98dp"
                android:layout_height="40dp"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="20dp"
                android:text="@string/membership_active_until"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewSurname" />

            <TextView
                android:id="@+id/textViewMembershipIWUT"
                android:layout_width="115dp"
                android:layout_height="44dp"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="20dp"
                android:text="@string/membership_remaining_days"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewMembershipAU" />

            <TextView
                android:id="@+id/textViewLastP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="20dp"
                android:text="@string/last_paid"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewMembershipIWUT" />

            <TextView
                android:id="@+id/textViewTariff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="20dp"
                android:text="@string/tariff"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewLastP" />

            <TextView
                android:id="@+id/textViewNameMember"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="111dp"
                android:layout_marginTop="0dp"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewName"
                app:layout_constraintLeft_toRightOf="@+id/textViewName"
                app:layout_constraintTop_toTopOf="@+id/textViewName"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textViewSurnameMember"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="0dp"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewSurname"
                app:layout_constraintLeft_toRightOf="@+id/textViewSurname"
                app:layout_constraintTop_toTopOf="@+id/textViewSurname"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textViewMembershipActiveUntil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="56dp"
                android:layout_marginTop="0dp"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewMembershipAU"
                app:layout_constraintLeft_toRightOf="@+id/textViewMembershipAU"
                app:layout_constraintTop_toTopOf="@+id/textViewMembershipAU"
                app:layout_constraintVertical_bias="0.545" />

            <TextView
                android:id="@+id/textViewMembershipIsWorthUpTo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="38dp"
                android:layout_marginTop="0dp"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewMembershipIWUT"
                app:layout_constraintLeft_toRightOf="@+id/textViewMembershipIWUT"
                app:layout_constraintTop_toTopOf="@+id/textViewMembershipIWUT"
                app:layout_constraintVertical_bias="0.382" />

            <TextView
                android:id="@+id/textViewlastPaid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="89dp"
                android:layout_marginTop="0dp"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewLastP"
                app:layout_constraintLeft_toRightOf="@+id/textViewLastP"
                app:layout_constraintTop_toTopOf="@+id/textViewLastP"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textViewTariffMember"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="114dp"
                android:layout_marginTop="0dp"
                android:textColor="#fff"
                android:textSize="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewTariff"
                app:layout_constraintLeft_toRightOf="@+id/textViewTariff"
                app:layout_constraintTop_toTopOf="@+id/textViewTariff"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/imageViewSettingsMember"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="39dp"
                android:layout_marginTop="24dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@mipmap/ic_settings" />

            <TextView
                android:id="@+id/textViewMyGymTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="28dp"
                android:fontFamily="casual"
                android:text="@string/app_name"
                android:textColor="#fff"
                android:textSize="30dp"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewSingOutMember"
                android:layout_width="45dp"
                android:layout_height="41dp"
                android:layout_marginLeft="39dp"
                android:layout_marginTop="27dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/icons8_exit_sign_80" />
        </android.support.constraint.ConstraintLayout>
    </ScrollView>
</android.support.v4.widget.SwipeRefreshLayout>
</FrameLayout>
